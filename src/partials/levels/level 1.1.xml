<Level id="1.1">
	@import "../palettes/purple.xml"

	<grid type="base" width="1" height="2" symmetry="0">
		<!-- lines -->
		<i type="ns" x="0" y="0"></i>
		<i type="ns" x="0" y="1"></i>

		<i type="we" x="0" y="0"></i>
		<i type="we" x="0" y="1"></i>
		<i type="we" x="0" y="2"></i>

		<i type="ns" x="1" y="0"></i>
		<i type="ns" x="1" y="1"></i>

		<!-- start & exit -->
		<i type="start" x="0" y="2"></i>
		<i type="exit" x="1" y="0" d="0"></i>

		<!-- cell contents -->
		<i type="dot" x="0" y="0" c="1"></i>
		<i type="dot" x="0" y="1" c="2"></i>
	</grid>
</Level>
