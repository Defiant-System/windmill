		<i type="dot" x="0" y="2" c="2"></i>

		<i type="dot" x="1" y="0" c="1"></i>
		<i type="dot" x="1" y="1" c="2"></i>
		<i type="dot" x="1" y="2" c="2"></i>

		<i type="dot" x="2" y="0" c="1"></i>
