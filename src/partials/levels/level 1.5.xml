		<i type="ns" x="0" y="1"></i>
		<i type="ns" x="0" y="2"></i>

		<i type="we" x="0" y="0"></i>
		<i type="we" x="0" y="1"></i>
		<i type="we" x="0" y="2"></i>
		<i type="we" x="0" y="3"></i>

		<i type="ns" x="1" y="0"></i>
