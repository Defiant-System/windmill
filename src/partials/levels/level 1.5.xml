		<i type="we" x="1" y="2"></i>
		<i type="we" x="1" y="3"></i>

		<i type="ns" x="2" y="0"></i>
		<i type="ns" x="2" y="1"></i>
		<i type="ns" x="2" y="2"></i>

		<i type="we" x="2" y="0"></i>
